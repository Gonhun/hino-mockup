<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mockuphino">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MockupHino">
        <activity
            android:name=".DetailEbookActivity"
            android:exported="false"
            android:label="@string/title_activity_detail_ebook"
            android:theme="@style/Theme.MockupHino.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_ebook_list"
            android:theme="@style/Theme.MockupHino.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".EbookListActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MockupHino.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>